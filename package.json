{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker-dev": "docker-compose up --build --force-recreate",
    "docker-build": "docker-compose -f docker-compose.production.yml up --build --force-recreate",
